    "import math\n",
    "\n",
    "def get_perm(n, i):\n",
    "    return (math.factorial(n))/(math.factorial(i)*math.factorial(n-i))\n",
    "\n",
    "for i in range(11):\n",
    "    print(get_perm(11, i)%(2**3))\n"
   ]
